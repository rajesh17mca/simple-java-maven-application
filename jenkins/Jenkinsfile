pipeline {
  agent any

  stages {
    stage('build') {
      steps {
        sh 'mvn clean package'
      }

      post {
        success {
          archiveArtifacts artifacts: '**/target/*.war'
        }
      }
    }

    stage('test') {
      steps {
        echo "This is Testing stage"
      }
    }

    stage ('deploy') {
      steps {
        echi "Thhis is Deploy stage"
      }
    }
  }
}